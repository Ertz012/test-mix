{
    "topology": {
        "layers": [
            "entry",
            "intermediate",
            "exit"
        ],
        "nodes_per_layer": 12,
        "num_clients": 5,
        "num_providers": 3
    },
    "network": {
        "base_port": 8000,
        "host": "localhost",
        "packet_loss_rate": 0.0
    },
    "mix_settings": {
        "strategy": "poisson",
        "mu": 0.5
    },
    "features": {
        "dummy_traffic": false,
        "retransmission": true,
        "path_reestablishment": false,
        "backup_mixes": true,
        "parallel_paths": false,
        "anonymous_return_addresses": false,
        "layered_encryption": true,
        "mock_encryption": true
    },
    "traffic": {
        "rate_packets_per_sec": 5.0,
        "duration_sec": 600,
        "mode": "random",
        "precalc_file": "traffic_data.bin",
        "loop_rate_packets_per_sec": 1.0,
        "drop_rate_packets_per_sec": 1.0,
        "interval_sec": 0.1
    },
    "logging": {
        "level": "INFO",
        "log_dir": "logs"
    }
}